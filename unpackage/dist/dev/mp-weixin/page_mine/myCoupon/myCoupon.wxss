page {
  background-color: #f6f6f6;
}
.top {
  position: -webkit-sticky;
  position: sticky;
  height: 100rpx;
  z-index: 9999;
  background-color: #FFFFFF;
  border-bottom: 2rpx solid #efefef;
  padding-top: 27rpx;
  top: 0;
}
.top .subsection {
  width: 60%;
}
.card_list {
  padding: 15rpx;
}
.card_list .card_item {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.card_list .card_empty {
  padding: 50rpx;
}
.card_coupon {
  padding: 15rpx;
}
.card_coupon .card_item1 {
  display: flex;
  flex-direction: column;
}
.card_coupon .card_empty {
  padding: 50rpx;
}

