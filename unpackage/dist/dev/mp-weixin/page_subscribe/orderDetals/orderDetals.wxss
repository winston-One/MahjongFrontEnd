page {
  background-color: #f6f6f6;
}
.submit-order {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.submit-order .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  padding: 25rpx;
}
.submit-order .top .top_store {
  font-size: 15px;
  font-weight: 600;
}
.submit-order .room {
  display: flex;
  justify-content: flex-start;
  padding: 25rpx;
  background-color: #fff;
  margin-top: 15rpx;
}
.submit-order .room image {
  width: 280rpx;
  height: 200rpx;
}
.submit-order .room .room_info {
  font-size: 16px;
  font-weight: 500;
  margin-left: 25rpx;
}
.submit-order .order-msg {
  display: flex;
  flex-direction: column;
  justify-content: center;
  background-color: #fff;
  margin-top: 15rpx;
}
.submit-order .order-msg .flex-style {
  display: flex;
  justify-content: space-between;
  padding: 25rpx;
  height: 90rpx;
  border-bottom: 2px solid #f6f6f6;
}
.submit-order .order-msg .flex-style .reserve_time {
  display: flex;
  justify-content: flex-end;
}
.submit-order .order-msg .coupon-style {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  border-bottom: 2px solid #f6f6f6;
}
.submit-order .order-msg .coupon-style .coupon {
  display: flex;
  justify-content: space-between;
  padding: 15rpx;
  border: 2px solid #a3c3f9;
  border-radius: 20rpx;
}
.submit-order .order-msg .coupon-style .meituan {
  display: flex;
  justify-content: space-between;
  border: 2px solid #a3c3f9;
  padding: 15rpx;
  border-radius: 20rpx;
}

